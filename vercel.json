{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/frontend/browser",
        "includeFiles": ["dist/frontend/browser/**"]
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/frontend/browser/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ]
}